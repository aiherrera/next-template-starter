{
  "name": "next-template-starter",
  "version": "0.0.0",
  "private": true,
  "author": {
    "name": "aiherrera",
    "email": "aiherrera@users.noreply.github.com",
    "url": "https://aiherrera.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aiherrera/next-template-starter.git"
  },
  "keywords": [
    "nextjs",
    "react",
    "template",
    "husky",
    "eslint",
    "commitlint",
    "standard-version",
    "semantic-release",
    "codecov"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "check:types": "tsc --pretty --noEmit",
    "check:format": "pretty-quick --staged",
    "check:lint": "eslint . --ext .ts,.tsx,.js,.jsx pages --color",
    "check:all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "lint:staged": "lint-staged",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "dependencies": {
    "@cloudinary/react": "^1.13.0",
    "@cloudinary/url-gen": "^1.19.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@headlessui/react": "^2.0.4",
    "@heroicons/react": "^2.1.3",
    "@hookform/resolvers": "^3.6.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "20.14.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "autoprefixer": "10.4.19",
    "cmdk": "^1.0.0",
    "encoding": "^0.1.13",
    "eslint": "8.56.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-security": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-sonarjs": "^1.0.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "next": "14.2.3",
    "postcss": "8.4.38",
    "prettier": "^3.3.1",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "pretty-quick": "^4.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.51.5",
    "simple-zustand-devtools": "^1.1.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "3.4.4",
    "typescript": "5.4.5",
    "uuid": "^9.0.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "packageManager": "pnpm@9.2.0+sha512.98a80fd11c2e7096747762304106432b3ddc67dcf54b5a8c01c93f68a2cd5e05e6821849522a06fb76284d41a2660d5e334f2ee3bbf29183bf2e739b1dafa771"
}
