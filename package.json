{
  "name": "next-template-starter",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "aiherrera",
    "email": "aiherrera@users.noreply.github.com",
    "url": "https://aiherrera.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aiherrera/next-template-starter.git"
  },
  "keywords": [
    "nextjs",
    "react",
    "template",
    "husky",
    "eslint",
    "commitlint",
    "standard-version",
    "semantic-release",
    "codecov"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "check:types": "tsc --pretty --noEmit",
    "check:format": "pretty-quick --staged",
    "check:lint": "eslint . --ext .ts,.tsx,.js,.jsx pages --color",
    "check:all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "lint:staged": "lint-staged",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "dependencies": {
    "@cloudinary/react": "^1.11.2",
    "@cloudinary/url-gen": "^1.10.1",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "autoprefixer": "10.4.14",
    "cmdk": "^0.2.0",
    "encoding": "^0.1.13",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "next": "13.4.7",
    "postcss": "8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "pretty-quick": "^3.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.44.3",
    "simple-zustand-devtools": "^1.1.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3",
    "uuid": "^9.0.0",
    "zod": "^3.21.4",
    "zustand": "^4.3.8"
  }
}
